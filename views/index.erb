<h1>Welcome to CrouchingPythonBNB</h1>

<form action="/sign_out" method="get">
  <input type="submit" value="sign_out" />
</form>

<form action="/list_property" method="get">
  <input type="submit" value="List a Space" />
</form>

<ul>
<% @properties.each do |property| %>
<li>
  <%= property.name %>
  <%= property.description %>
  <%= property.price %>
</li>
<% end %>
</ul>
